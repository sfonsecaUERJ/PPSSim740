<?xml version="1.0"?>
<DDDefinition xmlns="http://www.cern.ch/cms/DDL" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.cern.ch/cms/DDL ../../DDLSchema/DDLSchema.xsd">
  <SpecParSection label="regions.xml" eval="true">


<SpecPar name="pps_silicon_det">
   <PartSelector path="//SIDET"/>
      <Parameter name="CMSCutsRegion" value="SidetRegion" eval="false"/>
      <Parameter name="ProdCutsForElectrons" value="100*mm"/>
      <Parameter name="ProdCutsForPositrons" value="100*mm"/>
      <Parameter name="ProdCutsForGamma" value="100*mm"/>
 </SpecPar>  
<SpecPar name="pps_quartic_det">
   <PartSelector path="//ToFDET"/>
      <Parameter name="CMSCutsRegion" value="ToFdetRegion" eval="false"/>
      <Parameter name="ProdCutsForElectrons" value="100*mm"/>
      <Parameter name="ProdCutsForPositrons" value="100*mm"/>
      <Parameter name="ProdCutsForGamma" value="100*mm"/>
 </SpecPar>  
<SpecPar name="pps_station">
   <PartSelector path="//PPS"/>
      <Parameter name="CMSCutsRegion" value="PPSRegion" eval="false"/>
      <Parameter name="ProdCutsForElectrons" value="100*mm"/>
      <Parameter name="ProdCutsForPositrons" value="100*mm"/>
      <Parameter name="ProdCutsForGamma" value="100*mm"/>
 </SpecPar>  

  </SpecParSection>
</DDDefinition>


